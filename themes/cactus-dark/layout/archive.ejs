<ul class="post-list">
  <% page.posts.sort('date', 'desc').each(function(post) { %>
    <li class="post-item">
      <%- partial('_partial/post/date', { post: post, class_name: 'meta' }) %>
      <span class="post-name"><%- partial('_partial/post/title', { post: post, index: true, class_name: '' }) %></span>
      <span class="read-time"> <%- readingTime(post.content, 'min read') %></span>
    </li>
  <% }); %>
</ul>
